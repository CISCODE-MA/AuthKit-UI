{
    "compilerOptions": {
        "target": "es2016",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "skipLibCheck": true,
        "jsx": "react",
        "module": "ESNext",
        "declaration": true,
        "declarationMap": true,
        "sourceMap": true,
        "outDir": "dist",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "emitDeclarationOnly": false
    },
    "include": ["src", "src/models", "src/vite-env.d.ts", "../template-fe-model-users/src/models/table"],
    "exclude": ["src/test/**/*", "test/**/*", "**/*.test.ts", "**/*.test.tsx", "**/*.spec.ts", "**/*.spec.tsx", "examples/**/*"]
}